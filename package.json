{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/redis": "^2.8.22",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.0",
    "http-status-codes": "^1.4.0",
    "jest": "^26.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "tedis": "^0.1.12",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.10",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  }
}
